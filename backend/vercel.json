{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "GOOGLE_SERVICE_ACCOUNT_JSON": "@google-service-account-json",
    "GOOGLE_SPREADSHEET_ID": "@google-spreadsheet-id"
  }
}
